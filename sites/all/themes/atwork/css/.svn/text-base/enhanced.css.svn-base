/* --------------------------------Media queries */
@media screen and (min-width: 480px) {
  body {
    background: #eee !important;
    font-family: Helvetica;
  }
  h1, h2, h3, h4, h5, h6 {
      word-wrap: break-word;
      -webkit-text-size-adjust: none;
  }
}